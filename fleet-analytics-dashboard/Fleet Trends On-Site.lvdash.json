{
  "datasets": [
    {
      "name": "642cfb65",
      "displayName": "Vehicle GPS Data",
      "queryLines": [
        "SELECT * FROM users.david_hurley.new_taxigpsdata"
      ]
    },
    {
      "name": "367d8d30",
      "displayName": "Vehicle In Critical Zone",
      "queryLines": [
        "WITH polygon AS (\n",
        "  SELECT ST_GeomFromText('POLYGON((-8.62 41.14, -8.6 41.14, -8.6 41.16, -8.62 41.16, -8.62 41.14))') AS geom\n",
        ")\n",
        "\n",
        "SELECT\n",
        "    t.taxi_id,\n",
        "    t.timestamp,\n",
        "    CAST(ST_Within(ST_Point(t.lon, t.lat), p.geom) AS INT) AS is_within_critical_zone\n",
        "FROM\n",
        "    users.david_hurley.new_taxigpsdata t\n",
        "CROSS JOIN\n",
        "    polygon p"
      ]
    }
  ],
