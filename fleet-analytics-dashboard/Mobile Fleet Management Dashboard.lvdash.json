{
  "datasets": [
    {
      "name": "642cfb65",
      "displayName": "Equipment Location and Congestion Zone Perimeter Data",
      "queryLines": [
        "-- get the bounding box for each congestion zone polygon\n",
        "WITH polygon_boundaries_min_max AS (\n",
        "    SELECT\n",
        "        congestion_zone_id,\n",
        "        ST_XMin(polygon_geom) AS xmin,\n",
        "        ST_XMax(polygon_geom) AS xmax,\n",
        "        ST_YMin(polygon_geom) AS ymin,\n",
        "        ST_YMax(polygon_geom) AS ymax\n",
        "    FROM \n",
        "        users.david_hurley.congestion_zone_polygon_geom_data\n",
        ")\n",
        "-- combine equipment GPS points and densified polygon points into one dataset\n",
        "SELECT \n",
        "    t1.route_id,\n",
        "    t1.equipment_id,\n",
        "    t1.timestamp,\n",
        "    t1.latitude,\n",
        "    t1.longitude,\n",
        "    NULL as congestion_zone_id\n",
        "FROM \n",
        "    users.david_hurley.taxi_gps_geom_data as t1\n",
        "UNION ALL\n",
        "SELECT \n",
        "    NULL AS route_id,\n",
        "    NULL AS equipment_id,\n",
        "    NULL AS timestamp,\n",
        "    st_y(st_transform(st_point(p.x, p.y, 3763), 4326)) AS latitude, -- convert from meters to lat/long\n",
        "    st_x(st_transform(st_point(p.x, p.y, 3763), 4326)) AS longitude,\n",
        "    congestion_zone_id\n",
        "FROM polygon_boundaries_min_max as t2\n",
        "LATERAL VIEW explode(\n",
        "    -- generate points every 5m alongs polygon boundary using UDF\n",
        "    users.david_hurley.densify_polygon_boundary(t2.xmin, t2.xmax, t2.ymin, t2.ymax, 5)\n",
        ") AS p; \n"
      ]
    },
    {
      "name": "367d8d30",
      "displayName": "Equipment In Congested Zone",
      "queryLines": [
        "SELECT\n",
        "  t1.*,\n",
        "  t2.congestion_zone_id,\n",
        "  CAST(ST_Contains(polygon_geom, point_geom) AS INT) AS is_equipment_in_congestion_zone\n",
        "FROM\n",
        "  users.david_hurley.taxi_gps_geom_data AS t1\n",
        "CROSS JOIN\n",
        "  users.david_hurley.congestion_zone_polygon_geom_data AS t2"
      ]
    },
    {
      "name": "3c0f4253",
      "displayName": "Equipment Proximity Alert",
      "queryLines": [
        "SELECT\n",
        "   c.timestamp,\n",
        "   c.equipment_id,\n",
        "   CASE WHEN COUNT_IF(ST_Intersects(c.geom, p.geom)) > 0 THEN 1 ELSE 0 END AS is_equipment_to_close,\n",
        "   FILTER(COLLECT_LIST(p.equipment_id), x -> x != c.equipment_id) AS close_equipment_ids\n",
        "FROM (\n",
        "   SELECT\n",
        "       equipment_id,\n",
        "       timestamp,\n",
        "       ST_Buffer(point_geom, 5000) AS geom\n",
        "   FROM users.david_hurley.taxi_gps_geom_data\n",
        ") c\n",
        "LEFT JOIN (\n",
        "   SELECT\n",
        "       equipment_id,\n",
        "       timestamp,\n",
        "       ST_Buffer(point_geom, 5000) AS geom\n",
        "   FROM users.david_hurley.taxi_gps_geom_data\n",
        ") p\n",
        "ON c.timestamp = p.timestamp\n",
        "AND c.equipment_id != p.equipment_id\n",
        "GROUP BY c.timestamp, c.equipment_id\n",
        "HAVING COUNT_IF(ST_Intersects(c.geom, p.geom)) > 0"
      ]
    }
  ],
